                task.wait(0.5)

                for i, pos in ipairs(HanamiPositions) do
                    if not MTHanamiloop then break end
                    hrp.CFrame = CFrame.new(pos)
                    task.wait(3)

                    if i == #HanamiPositions then
                        task.wait(3) 
                        hrp.CFrame = CFrame.new(HanamiPositions[1])
                        break 
                    end
                end
            end
        end)
    end
